# DO NOT COMMIT THIS FILE WITH REAL CREDENTIALS
# Create the actual secret with:
# kubectl create secret generic s3-credentials \
#   --from-literal=access-key-id=YOUR_DO_SPACES_KEY \
#   --from-literal=secret-access-key=YOUR_DO_SPACES_SECRET \
#   --namespace=automlops

apiVersion: v1
kind: Secret
metadata:
  name: s3-credentials
  namespace: automlops
type: Opaque
stringData:
  access-key-id: "YOUR_DO_SPACES_KEY"
  secret-access-key: "YOUR_DO_SPACES_SECRET"
